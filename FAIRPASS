// START48 | Passes of Fair
import java.util.*;
class codechef
{
    public static void main (String[] args) {
        Scanner sc=new Scanner(System.in);
        int count= sc.nextInt();
        for(int i=0;i<count;i++)
        {
         int num1=sc.nextInt();
         int num2=sc.nextInt();
         if(num1<num2)
         {
             System.out.println("YES");
        }
        else{
            System.out.println("NO");
        }
    }
}
}
