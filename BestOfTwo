// Problem Solving
import java.util.Scanner;
class first
{
    public static void main (String[] args) {
        Scanner sc= new Scanner(System.in);
        int num= sc.nextInt();
        
        for(int i=1;i<=num;i++)
        {
            int num1= sc.nextInt();
            int num2= sc.nextInt();
            if(num1<num2)
                System.out.println(num2);
            else if(num1>num2)
                    System.out.println(num1);
            else
            System.out.println(num1);
        }
        
    }
}
